    "num=int(input(\"Enter a number: \"))\n",
    "rev=0\n",
    "while(num>0):\n",
    "    rem=num%10\n",
    "    rev=(rev*10)+rem\n",
    "    num=num//10\n",
    "print(\"The reverse number is {}\". format(rev))"
